{
  "presets": [
    ["@babel/preset-env", {
      "modules": false
    }],
    ["@babel/preset-react"]
  ],
  "plugins": [
    ["@babel/plugin-transform-class-properties"],
    ["@babel/plugin-proposal-export-default-from"]
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "modules": "commonjs"
        }],
        ["@babel/preset-react"]
      ]
    }
  }
}
 
